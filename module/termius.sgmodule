# Surge 配置片段

[MITM]
hostname = %APPEND% api.termius.com


[Body Rewrite]
http-response-jq ^https:\/\/api\.termius\.com\/api\/v4\/bulk\/account\/ 'if .account then .account.plan_type = "Pro" | .account.user_type = "Pro" end | if .access_objects then .access_objects |= map(if .title == "Trial" then .title = "Pro" else . end) end | if .trial and .trial.trial_type == "Individual" then .trial.trial_type = "Pro" else . end'


